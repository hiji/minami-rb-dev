<h1>Listing plans</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Starton</th>
    <th>Endon</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @plans.each do |plan| %>
  <tr>
    <td><%= plan.title %></td>
    <td><%= plan.starton %></td>
    <td><%= plan.endon %></td>
    <td><%= plan.address %></td>
    <td><%= link_to 'Show', plan %></td>
    <td><%= link_to 'Edit', edit_plan_path(plan) %></td>
    <td><%= link_to 'Destroy', plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Plan', new_plan_path %>
